<tr>
    <td>
        <input type="checkbox" data-id="{{id}}">
    </td>
    <td>
        {{name}}
    </td>

    <td class="action">
        <span class="price">{{price|money}}$</span>
    </td>
    <td class="action">
        <button class="bordered_red boridered">{{status}}</button>
    </td>
    <td class="action">
        <button class="cart" data-id="{{id}}" onclick="___mc.events.push('shop.bulk.cart.add', this)"></button>
    </td>
</tr>